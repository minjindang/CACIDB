                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT DISTINCT d.Aow_Code,
                                (SELECT          Comm_Name
                                  FROM               Committee
                                  WHERE           (Comm_Code = i.Comm_Code)
                                  GROUP BY    Comm_Name) AS CommName, j.Eval_TotScore, j.Eval_Note
FROM              Project AS b INNER JOIN
                            PjStage AS c ON b.Pj_Code = c.Pj_Code INNER JOIN
                            Allowance AS d ON b.Pj_Code = d.Pj_Code INNER JOIN
                            PjJudge AS i ON b.Pj_Code = i.Pj_Code LEFT OUTER JOIN
                            Evaluations AS j ON d.Aow_Code = j.Aow_Code AND i.Comm_Code = j.Comm_Code
WHERE          (1 = 1)</CommandText>
