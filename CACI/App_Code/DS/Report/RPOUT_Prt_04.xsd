        <Tables>
          <TableAdapter BaseClass="System.ComponentModel.Component" DataAccessorModifier="AutoLayout, AnsiClass, Class, Public" DataAccessorName="RPOUT_Prt_04TableAdapter" GeneratorDataComponentClassName="RPOUT_Prt_04TableAdapter" Name="RPOUT_Prt_04" UserDataComponentName="RPOUT_Prt_04TableAdapter">
            <MainSource>
              <DbSource ConnectionRef="CACIDB (Web.config)" DbObjectType="Unknown" FillMethodModifier="Public" FillMethodName="Fill" GenerateMethods="Both" GenerateShortCommands="false" GeneratorGetMethodName="GetData" GeneratorSourceName="Fill" GetMethodModifier="Public" GetMethodName="GetData" QueryType="Rowset" ScalarCallRetval="System.Object, mscorlib, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" UseOptimisticConcurrency="false" UserGetMethodName="GetData" UserSourceName="Fill">
                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT DISTINCT e.ApPj_ApGroup,
                                (SELECT          (CASE WHEN Com_Imple &lt;&gt; '' THEN Com_Name + '/' + Com_Imple ELSE Com_Imple END) 
                                                              AS Expr1
                                  FROM               Company
                                  WHERE           (Com_Code = f.Com_Code)) AS Com_Name,
                                (SELECT          Sys_CdText
                                  FROM               SysCode
                                  WHERE           (Sys_CdKind = 'P') AND (Sys_CdType = 'G') AND (Sys_CdCode = e.ApPj_ApGroup)) 
                            AS ApPj_ApGroup_Name, e.ApPj_Msectors,
                                (SELECT          Sys_CdText
                                  FROM               SysCode AS SysCode_1
                                  WHERE           (Sys_CdKind = 'I') AND (Sys_CdType = 'D') AND (Sys_CdCode = e.ApPj_Msectors)) 
                            AS ApPj_Msectors_Name, e.Aow_Code, e.ApPj_Name, e.ApPj_Goal, e.ApPj_Policies, e.ApPj_Profit, e.ApPj_Solution, 
                            e.ApPj_BgnDate, e.ApPj_EndDate, dbo.chgToChnDate(e.ApPj_BgnDate) AS ApPj_BgnDate_Roc, 
                            dbo.chgToChnDate(e.ApPj_EndDate) AS ApPj_EndDate_Roc, dbo.chgToChnChtDate(e.ApPj_BgnDate) 
                            AS ApPj_BgnDate_Roc_Cht, dbo.chgToChnChtDate(e.ApPj_EndDate) AS ApPj_EndDate_Roc_Cht, 
                            CONVERT(decimal(10, 2), e.ApPj_TotAmt / 1000, 0) AS ApPj_TotAmt, CONVERT(decimal(10, 2), e.ApPj_AowAmt / 1000, 
                            0) AS ApPj_AowAmt, dbo.chgToChnDate(GETDATE()) AS printDate, a.Comm_Code, e.Aow_Code AS uni_id,
                                (SELECT          CONVERT(decimal(10, 2), AVG(Evaluations.Eval_TotScore), 0) AS Expr1
                                  FROM               Evaluations LEFT OUTER JOIN
                                                              MtgTimes ON MtgTimes.Meeting_Code = Evaluations.Meeting_Code AND 
                                                              MtgTimes.Meeting_Index = Evaluations.Meeting_Index
                                  WHERE           (Evaluations.Comm_Code = a.Comm_Code) AND (Evaluations.Aow_Code = d.Aow_Code)) 
                            AS Eval_TotScore_avg
FROM              PjJudge AS a LEFT OUTER JOIN
                            Project AS b ON b.Pj_Code = a.Pj_Code LEFT OUTER JOIN
                            PjStage AS c ON c.Pj_Code = a.Pj_Code LEFT OUTER JOIN
                            Allowance AS d ON d.Pj_Code = a.Pj_Code LEFT OUTER JOIN
                            ApPjContext AS e ON e.Aow_Code = d.Aow_Code INNER JOIN
                            Evaluations AS f ON f.Aow_Code = e.Aow_Code
WHERE          (e.Aow_Code IS NOT NULL) AND (1 = 1)</CommandText>
                    <Parameters />
                  </DbCommand>
                </SelectCommand>
              </DbSource>
