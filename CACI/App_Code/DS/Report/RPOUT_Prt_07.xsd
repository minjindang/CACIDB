                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>select
	Allowance.Aow_Code
	,Company.Com_Code
	,Company.Com_Name
	,Allowance.Aow_PJPM
	,ApPjContext.ApPj_Name
	,ApplyAsis.Com_Code ApplyAsis_Com_Code
	,(select Com_Name from Company where Com_Code = ApplyAsis.Com_Code)ApplyAsis_Com_Name
	,ApPjContext.ApPj_ApGroup
	,(select Sys_CdText from SysCode where Sys_CdKind = 'P' and Sys_CdType = 'G' and Sys_CdCode = ApPjContext.ApPj_ApGroup)ApPj_ApGroup_Name
	,ApPjContext.ApPj_ProdCls
	,(select Sys_CdText from SysCode where Sys_CdKind = 'A' and Sys_CdType = 'P' and Sys_CdCode = ApPjContext.ApPj_ProdCls)ApPj_ProdCls_Name
	,ApPjContext.ApPj_Msectors
	,(select Sys_CdText from SysCode where Sys_CdKind = 'I' and Sys_CdType = 'D' and Sys_CdCode = ApPjContext.ApPj_Msectors)ApPj_Msectors_Name
	,Company.Com_CttName
	,Company.Com_CttTel
	,Company.Com_CttTel2
	,Company.Com_CttMail
	,Company.Com_OPAddr
	,ApPjContext.ApPj_AowAmt
	,ApPjContext.ApPj_FundAmt
	,ApPjContext.ApPj_TotAmt
	,dbo.chgToChnDate(GETDATE())printDate
	,ApPjContext.Aow_Code+PjStage.Pj_Code+cast(AowStage.Stage_Index as varchar)+cast(PjStage.Stage_Index as varchar) as uni_id
from PjStage
inner join AowStage on AowStage.Pj_Code = PjStage.Pj_Code
inner join Allowance on Allowance.Aow_Code = AowStage.Aow_Code
inner join ApPjContext on ApPjContext.Aow_Code = Allowance.Aow_Code
inner join ApplyAsis on ApplyAsis.Aow_Code = Allowance.Aow_Code
inner join Company on Company.Com_Code = Allowance.Com_Code
	and Company.Com_Code = ApplyAsis.Com_Code</CommandText>
