                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>select
	Committee.Comm_Code
	,Evaluations.Meeting_Code
	,Committee.Comm_Name
from Committee
inner join Evaluations on Evaluations.Comm_Code = Committee.Comm_Code
