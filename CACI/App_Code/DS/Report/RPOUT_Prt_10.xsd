                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT          a.Aow_Code, b.Com_Imple, c.ApPj_Name, c.ApPj_BgnDate, c.ApPj_EndDate, b.Com_CttName, b.Com_CttTel, 
                            b.Com_CttCell, b.Com_Fax, b.Com_CttMail, b.Com_OPAddr,
                                (SELECT          y.Sys_CdText
                                  FROM               AowStage AS z INNER JOIN
                                                              SysCode AS y ON z.AwSg_Verify = y.Sys_CdCode
                                  WHERE           (z.Stage_Index = @Stage_Index_S) AND (z.Aow_Code = a.Aow_Code) AND (y.Sys_CdKind = 'S') AND 
                                                              (y.Sys_CdType = 'S')) AS firstResult,
                                (SELECT          y.Sys_CdText
                                  FROM               AowStage AS z INNER JOIN
                                                              SysCode AS y ON z.AwSg_Verify = y.Sys_CdCode
                                  WHERE           (z.Stage_Index = @Stage_Index_E) AND (z.Aow_Code = a.Aow_Code) AND (y.Sys_CdKind = 'S') AND 
                                                              (y.Sys_CdType = 'S')) AS finalResult, CAST(ROUND(CONVERT(float(10), a.Aow_Approved / 1000), 0) 
                            AS VARCHAR(10)) AS finalFee
FROM              Allowance AS a INNER JOIN
                            Company AS b ON a.Com_Code = b.Com_Code INNER JOIN
                            ApPjContext AS c ON a.Aow_Code = c.Aow_Code INNER JOIN
                            Project AS d ON a.Pj_Code = d.Pj_Code
WHERE          (d.Pj_Code = @Pj_Code)</CommandText>
