                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>select dbo.chgToChnDate(convert(nvarchar(10),getdate(),111)) as getdate, b.Com_Name,    --單位名稱
       b.Com_Tel,                 --單位電話
       b.Com_CttName,            --聯絡人
       b.Com_CttCell,            --聯絡人手機
       b.Com_CttMail,            --聯絡人電子郵件
       b.Com_CttName2,           --聯絡人
       b.Com_CttCell2,           --聯絡人手機
       b.Com_CttMail2,           --聯絡人電子郵件
       b.Com_OPAddr,             --單位住址
       c.Pj_Name                 --專案名稱
from Coach a inner join Company b on a.Com_Code = b.Com_Code 
     inner join Project c on a.Pj_Code = c.Pj_Code
--查詢條件有填：統編/身份證字號/立案號 需加入↓
group by b.Com_Name,b.Com_Tel,b.Com_CttName,b.Com_CttCell,b.Com_CttMail,b.Com_CttName2,b.Com_CttCell2,b.Com_CttMail2,b.Com_OPAddr,b.Com_Code,c.Pj_Name
order by b.Com_Code
</CommandText>
