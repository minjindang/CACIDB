--申請本會經費
       b.Com_FinancialClass   --縣市財力分級(缺)
from Allowance a inner join Company b on a.Com_Code = b.Com_Code
     inner join ApPjContext c on a.Aow_Code = c.Aow_Code
where a.Aow_Code = ('申請之案件編號')
</CommandText>
